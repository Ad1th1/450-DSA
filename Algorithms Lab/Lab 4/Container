#include<bits/stdc++.h>
using namespace std;

void load_container(int maxw,vector<int> weight,int n){
    sort(weight.begin(),weight.end());
    int temp = maxw;
    vector<int> taken(n,0);
    int c=0;
    for(int i=0;i<n;i++){
        if(temp-weight[i]>=0){

            c++;
        }
        else
            break;

    }
    cout<<"Weights used = "<<c;

}

int main(){
    int maxw;
    cout<<"Enter max weight";
    cin>>maxw;
    int n;
    cout<<"Enter number of objects";
    cin>>n;
    vector<int>v;
    for(int i=0;i<n;i++){
        cin>>v[i];
    }
    load_container(maxw,v,n);
}
